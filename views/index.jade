extends layout

block content
  div.container
    div.site-wrapper
      div.masthead.animated.fadeIn
        div.nav
          ul
            li 
              a(href="#directions") Directions
            li
             a(href="#program") Program
            li
             a(href="#rsvp") RSVP
            li
             a(href="#registry") Registry
      div.animated.bounceInDown
        h1.text-center#heading Jon and Xiao
        div.non-semantic-protector
          div.ribbon
            strong.ribbon-content
              h1 05 . 24 . 15
      div.animated.fadeIn
        p After 8 years of being together, we are finally tying the knot. Thank you for taking the time to visit our wedding page. We are glad that to have known all of you, and friendship, companionship, witness of our love.  See you on the big day!
        p 2PM
        p ST FRANCIS OF ASSISI
        P 5111 SAN FELIPE RD, SAN JOSE, CA 95135
        P
        P RECEPTION AND DINNER TO FOLLOW
    
      div.program
        a(name="program") 
        include program


      div.directions
        a(name="directions")
        include directions

      div.rsvp
        a(name="rsvp") RSVP
      div.honeyfund
        a(name="registry") Registry

  script(type='text/javascript' src='https://code.jquery.com/jquery-2.1.1.min.js')
  script(type='text/javascript' src='https://hammerjs.github.io/dist/hammer.min.js')
  script(type='text/javascript' src="http://cdnjs.cloudflare.com/ajax/libs/jquery-localScroll/1.3.5/jquery.localScroll.min.js")
  script(type='text/javascript' src="http://cdnjs.cloudflare.com/ajax/libs/jquery-scrollTo/1.4.11/jquery.scrollTo.min.js")
  script(type='text/javascript' src='/Script/index.js')

