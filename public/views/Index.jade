extends layout

block content
  div.container
    div.site-wrapper
      div.masthead.animated.fadeIn
        include nav
     
      div.announcement
        a(name="announcement")
        include announcement
    
      div.program
        a(name="program") 
        include program

      div.directions
        a(name="directions")
        include directions

      div.rsvp
        a(name="rsvp") RSVP
      div.honeyfund
        a(name="registry") Registry

  script(type='text/javascript' src='https://code.jquery.com/jquery-2.1.1.min.js')
  script(type='text/javascript' src='https://hammerjs.github.io/dist/hammer.min.js')
  script(type='text/javascript' src="https://cdnjs.cloudflare.com/ajax/libs/jquery-localScroll/1.3.5/jquery.localScroll.min.js")
  script(type='text/javascript' src="https://cdnjs.cloudflare.com/ajax/libs/jquery-scrollTo/1.4.11/jquery.scrollTo.min.js")
  script(type='text/javascript' src='/Script/index.js')

